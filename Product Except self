
    vector<int> productExceptSelf(std::vector<int>& nums) {
    int n=nums.size();
    vector<int>products(n,1);
    int leftproduct=1;
    for(int i=0;i<n;i++)
    {
     products[i]*=leftproduct;
     leftproduct*=nums[i];
    }
    int rightproduct=1;
    for(int i=n-1;i>=0;i--)
    {
     products[i]*=rightproduct;
     rightproduct*=nums[i];
    }
return products;
    }
